pmid = {25037511},
title = {{Introducing nonlinear time series analysis in undergraduate courses}},
year = {2006}
}
@article{Perc2006a,
abstract = {Dielectric relaxation spectra of two closely related glass formers,$\backslash$ndipropylene glycol [H-(C3H6O)(2)-OH] and dipropylene glycol dimethyl$\backslash$nether [CH3-O-(C3H6O)(2)-CH3], were measured at ambient and elevated$\backslash$npressures in the supercooled and the glassy states are presented.$\backslash$nHydrogen bonds formed in dipropylene glycol are removed when its$\backslash$nends are replaced by two methyl groups to become dipropylene glycol$\backslash$ndimethyl ether. In the process, the primary relaxation, the excess$\backslash$nwing, and the resolved secondary relaxation of dipropylene glycol$\backslash$nare all modified when the structure is transformed to become dipropylene$\backslash$nglycol dimethyl ether. The modifications include the pressure and$\backslash$ntemperature dependences of these relaxation processes and their interrelations.$\backslash$nThus, by comparing the dielectric spectra of these two closely related$\backslash$nglass formers at ambient and elevated pressures, the differences$\backslash$nin the relaxation dynamics and properties in the presence and absence$\backslash$nof hydrogen bonding are identified. (c) 2006 American Institute of$\backslash$nPhysics.},
