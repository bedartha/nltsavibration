isbn = {0930-7575},
issn = {09307575},
journal = {Climate Dynamics},
mendeley-groups = {NLTSA in Titles},
primaryClass = {nlin},
title = {{Comparing modern and Pleistocene ENSO-like influences in NW Argentina using nonlinear time series analysis methods}},
year = {2003}
}
