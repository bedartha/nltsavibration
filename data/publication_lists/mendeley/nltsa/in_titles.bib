keywords = {*Neural Networks (Computer),*Nonlinear Dynamics,Cybernetics,Logistic Models,Probability Learning,Time},
mendeley-groups = {NLTSA in Titles},
pmid = {8823629},
title = {{Nonlinear time series analysis by neural networks: a case study}},
year = {1996}
}
