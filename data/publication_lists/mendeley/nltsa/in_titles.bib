keywords = {Econophysics,Exchange rates,Nonlinear dynamics,Recurrence quantification},
mendeley-groups = {NLTSA in Titles},
title = {{Application of nonlinear time series analysis techniques to high-frequency currency exchange data}},
year = {2002}
}
