mendeley-groups = {NLTSA in Titles},
pmid = {20337020},
title = {{[Fetal electrocardiogram extraction based on nonlinear time series analysis]}},
year = {2010}
}
@article{Gotoda2010,
abstract = {The authors experimentally investigated the dynamic behavior of$\backslash$nbuoyancy-induced flame oscillation under a swirling flow produced by$\backslash$nrotating a cylindrical burner tube, focusing on the characterization of$\backslash$nthe complex dynamics in flame front by a use of nonlinear time series$\backslash$nanalysis. A Wayland method (R. Wayland et al., 1993), which quantifies$\backslash$nthe degree of parallelism of trajectories in phase space constructed$\backslash$nfrom time series data of flame front fluctuations, is applied as a$\backslash$nsophisticated nonlinear time series analysis in this work. To reveal$\backslash$nwhether or not the complex dynamics is deterministic chaos, a$\backslash$nquantitative method for discussing the null hypothesis that the$\backslash$nirregular components of the flame front fluctuations represent a$\backslash$nstochastic process (i.e., a surrogate data method; T. Schreiber and A.$\backslash$nSchmitz, 1996), is applied in this work. A sophisticated nonlinear time$\backslash$nseries analysis in combination with a surrogate data method, which has$\backslash$nnot been widely applied to the study of combustion phenomena, clearly$\backslash$ndemonstrates that the dynamic behavior undergoes a significant$\backslash$ntransition from periodic oscillation to low-dimensional deterministic$\backslash$nchaos with increasing rotational Reynolds number.},
author = {Gotoda, Hiroshi and Asano, Yuta and Chuah, Keng Hoo and Kushida, Genichiro and Miyano, Takaya},
doi = {10.1080/00102202.2010.498667},
issn = {00102202},
journal = {Combustion Science and Technology},
keywords = {Buoyancy,Chaos,Diffusion flame,Flickering,Nonlinear time series analysis,Surrogate data method,Swirling flow},
mendeley-groups = {NLTSA in Titles},
title = {{Dynamic behavior of buoyancy-induced flame oscillation under swirling flow by a use of nonlinear time series analysis in combination with surrogate data method}},
year = {2010}
}
@inproceedings{Georgakaki2010,
abstract = {Atomic Force Microscopy (AFM) is a widely used tool for nanoscale surface analysis. Dynamic atomic force microscopy (tapping or non-contact mode) consists of a vibrating microcantilever with a nanoscale tip that interacts with a sample surface via short- and long-range intermolecular forces Due to the need of high precision measurements, the interaction forces between the nanotip and the specimen must be carefully examined and analyzed in order to best evaluate the nonlinear dynamical response of the AFM system. Recent research on AFM systems has focused on detailed numerical analysis such that possible chaotic regions can be well defined. In this paper, nonlinear time-series analysis methods (NTSA) are applied to analyze signals generated by a numerical single-degree-of-freedom lumped-model of a dynamic-mode AFM. The signal characteristics will provide useful insight of the nonlinear system response leading in future work, to the evaluation of deterministic uncertainty in real-time AFM measurements that is being introduced because of these complex phenomena. The confirmation of chaos existence and the prediction of the regions where chaotic motion is possible remain crucial for the quality of AFM images. The methods used for the system analysis include Autocorrelation Function, Power Spectral Density Analysis, Phase-space analysis, Poincare Maps and Lyapunov Indicator All the above computational techniques can then be applied to experimental AFM data in order to find system parameters and regimes, which ensure stable motions across the sample surface scanning process.},
author = {Georgakaki, D. and Mitsas, Ch and Polatoglou, H. M.},
booktitle = {AIP Conference Proceedings},
doi = {10.1063/1.3322334},
isbn = {9780735407404},
issn = {0094243X},
keywords = {AFM,Dynamic mode,Dynamical system,Time series analysis},
mendeley-groups = {NLTSA in Titles},
title = {{Nonlinear time-series analysis methods for the evaluation {\&} study of dynamical phenomena in atomic force microscopy prediction of chaotic motion}},
year = {2010}
}
@article{Beslagic2010,
abstract = {We study the route from basic tones towards sounds and chords in terms of dynamical complexity of the underlying time series of the sound recordings. Although this route offers increasingly complex waveforms for the analysis, we provide conclusive evidences that even the most complex of them is still periodic. The employed methods of nonlinear time series analysis leading to this conclusion are very instructive, showing how to avoid pitfalls and false claims when trying to distinguish experimentally obtained chaotic traces from those that merely incorporate a large number of harmonic and inharmonic frequencies yet are still periodic. We use methods of nonlinear time series analysis as well as waveforms outputted by a frequency generator and actual recordings of picked guitar strings for the analysis, thus employing resources that are readily available in the lab as well as classrooms.},
author = {Be{\v{s}}lagi{\'{c}}, Said and Perc, Matja{\v{z}}},
issn = {15614085},
journal = {Nonlinear Phenomena in Complex Systems},
keywords = {Chaos,Quasiperiodicity,Stringed instruments,Times series analysis},
mendeley-groups = {NLTSA in Titles},
title = {{Cautionary example of nonlinear time series analysis: From tones to sounds}},
year = {2010}
}
@article{Liu2010,
abstract = {This paper presents a nonlinear time series analysis method for rotating machine damage detection and diagnostics. Specifically, the permutation entropy is investigated as a statistical measure for signal characterization. Through space reconstruction, the permutation entropy describes the complexity of the time series measured on a physical system, and takes its non-linear behavior into account. By identifying changes in the vibration signals measured on rotating machines, which are typical precursors of defect occurrence, permutation entropy can serve as a diagnostic tool. Experiments on a custom-designed gearbox system have confirmed its effectiveness for machine structural health monitoring applications. Copyright {\textcopyright} 2010 by ASME.},
author = {Liu, Yongbin and Gao, Robert X},
doi = {10.1115/DSCC2010-4118},
isbn = {9780791844182},
journal = {Time},
keywords = {DSCC2010-4118},
mendeley-groups = {NLTSA in Titles},
title = {{DSCC2010- A NONLINEAR TIME SERIES ANALYSIS METHOD FOR HEALTH MONITORING OF ROLLING}},
year = {2010}
}
@article{Tian2010,
abstract = {Non-linear time series analysis and its application in the quantification of dynamic stability were discussed, and VICON MX 3D motion capture system was used to collect the dynamic data of hip, knee and ankle when human were walking on treadmill at 40{\%}, 60{\%}, and 80{\%} of the Froude velocity. Matlab was used to realize the calculation of delay time $\tau$ through C-C method and the calculation of embedding dimension m through false-nearest-neighbor analysis, and finally the largest Lyapunov exponents $\lambda$ max  under different speeds was calculated. Then $\lambda$ max  was adopted as a quantified index for stability to study the relationship between speed and dynamic stability. The smaller $\lambda$ max  valued, the better stability was. The results show that $\lambda$ max  increases linearly and stability decreases with the increase of speed.},
author = {Tian, W.-J. and Jin, J.-F. and Cong, Q.},
issn = {16715497},
journal = {Jilin Daxue Xuebao (Gongxueban)/Journal of Jilin University (Engineering and Technology Edition)},
keywords = {Engineering bionics,Gait,Lyapunov exponents,Stability},
mendeley-groups = {NLTSA in Titles},
title = {{Application of nonlinear time series analysis on dynamic stability}},
year = {2010}
}
@inproceedings{VanLiet2010,
abstract = {In this paper, the fluctuations of the geomagnetic horizontal field measured at Hoc Mon Observatory (100 50′51″N, 106 033′36″E, suburb of Ho Chi Minh City) are studied using the nonlinear time series analysis. First, the autocorrelation and the average mutual information function (AMI) are calculated to estimate to delay time. Second, the false nearest neighbors are computed to estimate the embedding dimension, hence the reconstruction of the phase space. Thence the correlation dimension, the largest Lyapunov value and the short-term prediction are computed to demonstrate the deterministic phenomena of the fluctuation of the geomagnetic horizontal field in Ho Chi Minh City (in the low latitude).},
