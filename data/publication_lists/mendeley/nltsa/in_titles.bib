keywords = {Empirical mode decomposition,Hilbert transformation,Nonlinear time series},
mendeley-groups = {NLTSA in Titles},
title = {{On performance differences of EMD and WD in the nonlinear time series analysis}},
year = {2011}
}
@inproceedings{Hu2012a,
abstract = {It is human nature to swing their arms at the frequency of leg motion during walking, but the contribution of arm swing to dynamic stability of human motion segments was poorly understood. Based on the nonlinear time series analysis method, the objective of this study was to investigate the effects of arm swing in three conditions (natural, active and restricted arm swing) on the dynamic stability of spine and lower extremity joints, and to further assess the contribution of arm swing to the human dynamic stability in relation with age. Gait experiments were carried out for 10 young and 8 middle-aged healthy volunteers while walking with natural, active and restricted arm swing. The maximum finite time lyapunov exponents were calculated to quantify the local dynamic stability of spine and lower extremity joints under three arm swing conditions, and the percentage change of the maximum Lyapunov exponents was compared between two groups to evaluate the effectiveness of active arm swing in relation with age. For both young and middle-aged groups, no significant difference of the maximum lyapunov exponents of all motion segments was found between walking with natural arm swing and with restricted arm swing (P{\textgreater}0.05). However, the maximum lyapunov exponents of all motion segments while walking with active arm swing was significantly lower than those while walking with natural arm swing and restricted arm swing, respectively (P{\textless}0.05), and the percentage decrease of the maximum lyapunov exponents for all motion segments while walking with active arm swing was significantly higher in middle-aged group than in young group (P{\textless}0.05). These results indicated that active arm swing would help to improve dynamic stability of human motion segments, especially more effective with age.},
author = {Hu, Fei and Gu, Dong Yun and Chen, Jin Ling and Wu, Yu and An, Bing Chen and Dai, Ke Rong},
booktitle = {Proceedings of the Annual International Conference of the IEEE Engineering in Medicine and Biology Society, EMBS},
doi = {10.1109/EMBC.2012.6347075},
isbn = {9781424441198},
issn = {1557170X},
mendeley-groups = {NLTSA in Titles},
pmid = {23367009},
title = {{Contribution of arm swing to dynamic stability based on the nonlinear time series analysis method}},
year = {2012}
}
@article{KEH2012,
abstract = {Three methods of nonlinear time series analysis, Lempel-Ziv complexity,$\backslash$nprediction error and covariance complexity were employed to distinguish$\backslash$nbetween the electroencephalograms (EEGs) of normal children, children$\backslash$nwith mild autism, and children with severe autism. Five EEG tracings per$\backslash$ncluster of children aged three to seven medically diagnosed with mild,$\backslash$nsevere and no autism were used in the analysis. A general trend seen was$\backslash$nthat the EEGs of children with mild autism were significantly different$\backslash$nfrom those with severe or no autism. No significant difference was$\backslash$nobserved between normal children and children with severe autism. Among$\backslash$nthe three methods used, the method that was best able to distinguish$\backslash$nbetween EEG tracings of children with mild and severe autism was found$\backslash$nto be the prediction error, with a t-Test confidence level of above$\backslash$n98{\%}.},
