issn = {16744527},
journal = {Research in Astronomy and Astrophysics},
keywords = {Accretion, accretion disks,X-rays: binaries},
mendeley-groups = {NLTSA in Titles},
title = {{Nonlinear time series analysis of the light curves from the black hole system GRS1915+105}},
year = {2011}
}
@inproceedings{Wang2011d,
abstract = {The procedure of the Hilbert transformation (HT) based on empirical mode decomposition (EMD) is first decomposing a nonlinear time series into its intrinsic mode functions (IMFs), and then taking the HT of each IMF and computing the instantaneous amplitude and frequency. In the context of an ideal time series, merits and defects of EMD and wavelet decomposition (WD) as well as HT and wavelet transformation (WT) in the nonlinear time series analysis are systematically analyzed/compared in this paper, and aiming at their defects, some proposals for possible improvement are also given. Research results show that the combination with the EMD-based analysis method and the WD-based one may more effectively identify the characteristic information of the original time series.},
author = {Wang, Xiaojuan and Feng, Guolin and Feng, Aixia},
booktitle = {Proceedings - 2011 8th International Conference on Fuzzy Systems and Knowledge Discovery, FSKD 2011},
