title = {{Nonlinear time series analysis of dissolved oxygen in the Orbetello Lagoon (Italy)}},
year = {2007}
}
@article{King2007,
abstract = {Evidence suggests that dab and rainbow trout are able to quickly adjust their food intake to an appropriate level when offered novel diets. In addition day-to-day and meal-to-meal food intake varies greatly and meal timing is plastic. Why this is the case is not clear: Food intake in fish is influenced by many factors, however the hierarchy and mechanisms by which these interact is not yet fully understood. A model of food intake may be helpful to understand these phenomena; to determine model type it is necessary to understand the qualitative nature of food intake. Food intake can be regarded as an autoregressive (AR) time series, as the amount of food eaten at time t will be influenced by previous meals, and this allows food intake to be considered using time series analyses. Here, time series data were analysed using nonlinear techniques to obtain qualitative information from which evidence for the hierarchy of mechanisms controlling food intake may be drawn. Time series were obtained for a group of dab and individuals and a group of rainbow trout for analysis. Surrogate data sets were generated to test several null hypotheses describing linear processes and all proved significantly different to the real data, suggesting nonlinear dynamics. Examination of topography and recurrence diagrams suggested that all series were deterministic and non-stationary. The point correlation dimension (PD2i) suggested low-dimensional dynamics. Our findings suggest therefore that any model of appetite should create output that is deterministic, non-stationary, low-dimensional and having nonlinear dynamics. {\textcopyright} 2006 Elsevier Ltd. All rights reserved.},
author = {King, J. W. and Ruohonen, K. and Grove, D. J. and Hammerstein, A.},
doi = {10.1016/j.jtbi.2006.10.025},
isbn = {0022-5193 (Print)
0022-5193 (Linking)},
issn = {00225193},
journal = {Journal of Theoretical Biology},
keywords = {Dab,Food intake,Limanda limanda (L.),Nonlinear dynamics,Nonlinear time series analysis,Oncorhyncus mykiss (walbaum),Rainbow trout},
mendeley-groups = {NLTSA in Titles},
pmid = {17198714},
title = {{Nonlinear time series analysis of food intake in the dab and the rainbow trout}},
year = {2007}
}
@article{Osterhage2007,
abstract = {The framework of the theory of nonlinear dynamics provides powerful$\backslash$nconcepts and algorithms to study complicated dynamics such as brain$\backslash$nelectrical activity (electroencephalogram, EEG). Although different$\backslash$ninfluencing factors render the use of nonlinear measures in a strict$\backslash$nsense problematic, converging evidence from various investigations now$\backslash$nindicates that nonlinear EEG analysis provides a means to reliably$\backslash$ncharacterize different states of physiological and pathophysiological$\backslash$nbrain function. We here focus on applications of nonlinear EEG analysis$\backslash$nin epileptology. Epilepsy affects more than 50 million individuals$\backslash$nworldwide - approximately 1{\%} of the world's population. The disease is$\backslash$ncharacterized by a recurrent and sudden malfunction of the brain that is$\backslash$ntermed seizure. Nonlinear EEG analysis techniques allow to reliably$\backslash$nidentify the seizure generating structure (epileptic focus) in different$\backslash$nareas of the brain even during seizure-free intervals, to disentangle$\backslash$ncomplex spatio-temporal interactions between the epileptic focus and$\backslash$nother areas of the brain, and to define a specific state predictive of$\backslash$nan impending seizure. Nonlinear EEG analysis provides supplementary$\backslash$ninformation about the epileptogenic process in humans, contributes to an$\backslash$nimprovement of the presurgical evaluation of epilepsy patients, and$\backslash$noffers a basis for the development of new therapy concepts for seizure$\backslash$nprevention.},
author = {Osterhage, Hannes and Lehnertz, Klaus},
doi = {10.1142/S0218127407019081},
issn = {0218-1274},
journal = {International Journal of Bifurcation and Chaos},
keywords = {epilepsy,synchronization,time series analysis},
mendeley-groups = {NLTSA in Titles},
title = {{Nonlinear Time Series Analysis in Epilepsy}},
year = {2007}
}
@inproceedings{Han2007,
abstract = {Hyperspectral remotely sensed imagery is often modeled and processed by algorithms assuming that the imagery is a realization of a Gaussian linear stochastic process. These algorithms include some methods for feature extraction, spectral mixture analysis, and spatial analysis. The linear assumption, however, may not be realistic since there are factors that may introduce nonlinearities during the formulation of hyperspectral imagery. The existence of nonlinearity has a negative impact on the effectiveness and accuracy of information extraction. In this study, we propose a method to investigate the existence of nonlinearity in hyperspectral data, represented by a 4m AVIRIS image acquired over an area of coastal forests on Vancouver Island. The proposed method is based on a statistical test using surrogate data, an approach originally introduced in nonlinear time series analysis. High-order autocorrelations are used as the discriminating statistic to evaluate the differences between the hyperspectral data and their surrogates. Instead of conducting a statistical test in time domain as is used in typical time series analysis, we did it in spatial and spectral domains. The investigation revealed that the existence of nonlinearity in hyperspectral data is evident in spectral domain, but not in the spatial domain.},
author = {Han, Tian and Goodenough, David G.},
booktitle = {International Geoscience and Remote Sensing Symposium (IGARSS)},
doi = {10.1109/IGARSS.2007.4423107},
isbn = {1424412129},
keywords = {AVIRIS,Discriminating statistics,Hyperspectral,Nonlinearity,Remote sensing,Time series analysis},
mendeley-groups = {NLTSA in Titles},
title = {{Investigation of nonlinearity in hyperspectral remotely sensed imagery - A nonlinear time series analysis approach}},
year = {2007}
}
@article{Chelani2007,
abstract = {This study evaluates the potential of nonlinear time series analysis based methods in predicting the carbon monoxide concentration in an urban area. To establish the functional relationship between current and future observations, two models based on local approximations and neural network approximations are used. To compare the performance of the models, an autoregressive integrated moving average model is also applied. The multi-step forecasting capabilities of the models are evaluated. {\textcopyright} 2007 Elsevier Ltd. All rights reserved.},
