year = {2009}
}
@article{Cipollini2009,
abstract = {We analyze the fiscal adjustment process in the United States using a multivariate threshold vector error regression model. The shift from single-equation to multivariate setting adds value both in terms of our economic understanding of the fiscal adjustment process and the forecasting performance of nonlinear models. We find evidence that fiscal authorities intervene to reduce real per capita deficit only when it reaches a certain threshold and that fiscal adjustment takes place primarily by cutting government expenditure. The results of out-of-sample density forecast and probability forecasts suggest that a shift from a univariate autoregressive model to a multivariate model improves forecast performance.},
