journal = {Neuroscience Letters},
keywords = {Anesthesia,Artefact reduction,EEG,Pharmacokinetic and pharmacodynamic model,Recurrence quantification analysis,Sevoflurane},
mendeley-groups = {Recurrence Plots Library},
