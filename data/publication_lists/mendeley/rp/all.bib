@article{Jana2013a,
abstract = {Prey-predator system with prey refuge for the case of non-overlapping generations is considered here for our present study. Foraging efficiency of predator largely varies with the escaping ability of the prey from the predator. For this reason system experiences interesting and complex dynamical features with varying the strength of prey refuge, including both population extinction, predator extinction, stable coexistence, multiple invariant closed orbit in different chaotic regions, onset of chaos suddenly and sudden disappearance of the chaotic dynamics. In particular, we observe that when the prey is in stable, oscillatory or even chaotic status, then the predator can tend to extinction. Also, system experiences Hopf-bifurcation and flip bifurcations. Numerical computation is also performed to validate and visualize different theoretical results. The computations of Lyapunov exponent, fractal dimension of the map, recurrence plot and power spectral density confirm the chaotic dynamical behaviors. The analysis and results in this paper are interesting both in mathematics and biology. {\textcopyright} 2013 Elsevier Inc. All rights reserved.},
author = {Jana, Debaldev},
doi = {10.1016/j.amc.2013.09.001},
issn = {00963003},
journal = {Applied Mathematics and Computation},
keywords = {Chaos,Flip bifurcation,Hopf-bifurcation,Invariant closed curve,Predator-Prey interaction,Prey refuge},
mendeley-groups = {Recurrence Plots Library},
title = {{Chaotic dynamics of a discrete predator-prey system with prey refuge}},
year = {2013}
}
@article{2013a,
abstract = {The field of vibration-based structural health monitoring involves extracting a feature which robustly quantifies damage-induced changes of the structural dynamics. Generally, the change of structural characteristics due to damage is analyzed by observing frequencies, mode shapes, damping, etc. However, the change of those modal features due to damage is minor, so that detecting the location of damage is difficult. Recently, an attractor-based monitoring system has been proposed. The recent study using chaotic excitation has demonstrated that the change of the attractor caused by damage is more sensitive than that of the most commonly used frequency and mode shape of the structure, in which the recurrence plot was applied successfully to the damage detection. However, damage localization has not been discussed sufficiently. An attempt in this study was made to develop a damage localization method using chaotic excitation and recurrence quantification analysis. It was demonstrated through experiment results that the proposed system makes it possible to detect damage locations.},
author = {野村泰稔 and 森本大貴 and 日下貴之 and 古田均},
doi = {10.1299/kikaic.79.4210},
issn = {1884-8354},
journal = {日本機械学会論文集 C編},
keywords = {attractor,chaos,recurrence plots,structural damage localization},
mendeley-groups = {Recurrence Plots Library},
title = {カオス応答アトラクタのリカレンスプロットに基づく構造物の損傷位置同定},
year = {2013}
}
@article{Zhang2013b,
abstract = {Atrial fibrillation (AF) detection is important in clinical therapy. In this paper, an AF detection method is proposed based on Laplacian spectrum analysis of complex network. Firstly, a complex network named FRCN is constructed based on fuzzy symbolic dynamics (FSD) and recurrence plot. Then, with the calculated Laplacian matrix of FRCN, the Laplacian spectrum features are calculated as the features in the further classification. Finally, epicardium signals are classified into AF and normal sinus rhythm (NSR) classes using the fuzzy c-means algorithm (FCM). The method is validated with epicardium signals from three dogs named A, B and C. The signals are cut into small segments as the samples. The length of the sample is 1s. The sensitivity, specificity and accuracy of the identification for set A are 97.9{\%}, 99.3{\%} and 98.8{\%} respectively, 95.9{\%}, 98.6{\%}, 97.3{\%} for set B, and 96.8{\%}, 98.7{\%}, 97.8{\%} for set C. This experiment indicates that our approach is effective in AF detection.},
author = {Zhang, Y. and Wang, Y. and Yang, C. and Wu, X. and Qin, Y.},
issn = {02543087},
journal = {Yi Qi Yi Biao Xue Bao/Chinese Journal of Scientific Instrument},
keywords = {Atrial fibrillation,Complex network,Laplacian spectrum},
mendeley-groups = {Recurrence Plots Library},
title = {{Atrial fibrillation detection based on Laplacian spectrum analysis of complex network}},
year = {2013}
}
@article{HeeWai2013a,
abstract = {Snapping shrimp produce sounds through cavitation bubbles generated by the rapid closing of their claws. These sounds are a primary source of ambient noise in sub-tropical, shallow water environments. Though seasonal and daily variations of snapping shrimp sound levels have been reported, comprehensive studies of short time series have been limited. We investigate the respective spectral characteristics from acoustic arrays recordings at various locations in Oahu, {\{}HI{\}}, over hourly, daily and weekly periods. Nonlinear time series analysis methods encompassing recurrence plots are used to investigate the acoustic time series together with bivariate and cross recurrence plot analysis with that of tidal and sunlight cycles. Signal entropy and determinism are investigated and transient acoustic ship sounds are located},
author = {{Hee Wai}, Tyler and Allen, John S. and Gebbie, John and Siderius, Martin},
doi = {10.1121/1.4831199},
issn = {0001-4966},
journal = {The Journal of the Acoustical Society of America},
mendeley-groups = {Recurrence Plots Library},
title = {{Nonlinear time series analysis of snapping shrimp sounds}},
year = {2013}
}
@article{Jabonski2013a,
abstract = {Breathing is a fundamental physiological process produced by movements generated and controlled by efferent signals from the nervous system. Improving our understanding of the mechanisms underlying breathing in humans is of particular interest. Another important practical issue is the design of noninvasive procedures for the diagnosis, prediction, and control of the respiratory system, which works as a subsystem embedded in the complex physiological environment of the human organism and its external environment. This paper provides a concise review of a selected set of modern techniques dedicated to the exploration of complex and varying systems and sets of time-series data. These methods are based on an 1D entropic tool (approximate and sample entropy, i.e., ApEn and SampEn, respectively), which is effective for assessing the regularity and complexity of information contained in data sets, as well as complex network theory, recurrence plot (RP) strategy, and the joint complex network-recurrence analysis mode. Exemplary results are given for real physiological data recorded in patients with symptoms of central sleep apnea syndrome. Although ApEn and SampEn are shown to be sensitive methods for the detection of pathological mechanisms affecting breathing patterns during sleep, qualitative and quantitative studies based on the RP strategy reveal even better efficiency for this task. In addition, the second mode of analysis enables multi-dimensional correlation of accessible data, which is important for studying the couplings between numerous physiological subsystems. Further work in this area is proposed to map the scheme of breathing physiology during sleep.},
author = {Jab{\l}o{\'{n}}ski, I},
doi = {10.1109/JSEN.2013.2258257},
issn = {1530-437X VO  - 13},
journal = {IEEE Sensors Journal},
keywords = {1D entropic tool,Biomedical monitoring,RP strategy,breathing patterns,complex couplings,complex network theory,complex network-recurrence analysis mode,complex networks,complex physiological environment,computational complexity,data recording,data sets,human breathing,human organism,information complexity,medical disorders,multidimensional correlation,nervous system,neurophysiology,pathological mechanism detection,physiological data,physiological subsystems,pneumodynamics,recurrence plot strategy,respiratory system control,respiratory system diagnosis,respiratory system prediction,review,reviews,sleep,sleep apnea,sleep apnea syndrome,time series,time-series data},
mendeley-groups = {Recurrence Plots Library},
title = {{Modern Methods for the Description of Complex Couplings in the Neurophysiology of Respiration}},
year = {2013}
}
