isbn = {1539-3755},
issn = {15393755},
journal = {Physical Review E - Statistical, Nonlinear, and Soft Matter Physics},
mendeley-groups = {Recurrence Plots Library},
pmid = {20365442},
title = {{Identifying hidden common causes from bivariate time series: A method using recurrence plots}},
year = {2010}
}
@inproceedings{Wu2010c,
abstract = {We propose an approach to audio-based data-driven mu- sic visualization and an experimental design to study if the music visualization can aid listeners in identifying the structure of music. A three stage system is presented in- cluding feature extraction, the generation of a recurrence plot and the creation of an arc diagram to visualize the rep- etitions within a piece. Then subjects are asked to cate- gorize simple forms of classical music with and without audio and visual cues provided. The accuracy and speed are measured. The results show that the visualization can reinforce the identification of musical forms.},
author = {Wu, H.-H. and Bello, J P},
booktitle = {Proceedings of the SMC Conference 2010},
mendeley-groups = {Recurrence Plots Library},
title = {{Audio-based music visualization for music structure analysis}},
year = {2010}
}
@article{Stan2010a,
abstract = {The paper presents applications of the recurrence plot quantification to the analysis of type 1 intermittency observed in a discharge plasma. The main measures of recurrence plot analysis considered are the laminarity and the determinism. First, it is demonstrated that the laminarity measure can be related to the average time between chaotic bursts in this type of intermittency. Second, it is shown that the determinism measure introduced by the recurrence plot quantification depends on the control parameter as a decreasing exponential. The test of the results is performed by analyzing the determinism measure for type 1 intermittency observed in the logistic map. It is also shown that the role of the intrinsic noise of the plasma can easily be identified by this technique.},
author = {Stan, Cristina and Cristescu, C. P. and Dimitriu, D. G.},
doi = {10.1063/1.3385796},
issn = {1070664X},
journal = {Physics of Plasmas},
mendeley-groups = {Recurrence Plots Library},
title = {{Analysis of the intermittent behavior in a low-temperature discharge plasma by recurrence plot quantification}},
year = {2010}
}
@article{Renaud2010a,
abstract = {Sexual arousal and gaze behavior dynamics are used to characterize deviant sexual interests in male subjects. Pedophile patients and non-deviant subjects are immersed with virtual characters depicting relevant sexual features. Gaze behavior dynamics as indexed from correlation dimensions (D2) appears to be fractal in nature and significantly different from colored noise (surrogate data tests and recurrence plot analyses were performed). This perceptual-motor fractal dynamics parallels sexual arousal and differs from pedophiles to non-deviant subjects when critical sexual information is processed. Results are interpreted in terms of sexual affordance, perceptual invariance extraction and intentional nonlinear dynamics.},
author = {Renaud, P and Goyette, M and Chartier, S and Zhornitski, S and Trottier, D and Rouleau, J L and Proulx, J and Fedoroff, P and Bradford, J P and Dassylva, B and Bouchard, S},
