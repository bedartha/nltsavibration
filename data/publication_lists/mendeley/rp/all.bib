title = {{Stability criterion for aluminium alloy milling expressed by recurrence plot measures}},
year = {2012}
}
@article{Tahmasebpour2012a,
abstract = {Recurrence plot (RP) and recurrence quantification analysis (RQA), as powerful statistical techniques, have been used for studying the dynamic behavoir of gas-solids fluidized beds. The method of delays was used to reconstruct the state space attractor to carry out analysis in the reconstructed state space. In this work, variance of recurrence rate, which indictaes density of recurrence points in RP, against different epoch lengths (time windows) for time series of pressure fluctuation of fluidized bed was calculated. It was concluded that the characteristic parameters of RPs could reflect the extent of chaos in fluidization behaviour. The average cycle frequency and entropy as nonlinear dynamical invariants were evaluated with RQA at different aspect rations. The estimated entropy showed a similar trend of average cycle frequency for different aspect ratios. The results also showed that the entropy and average cycle frequency are higher in smaller aspect ratios showing that the importance of the finer structures. In addition, a minimum in average cycle frequency and entropy of the pressure fluctuations indicated a minimum deviation from periodicity or, in other words, a minimum deviation from the larger structures, of the bed. The results of this study allow the deep understanding the fluidized bed hydrodynamics which can then be used for scale up.},
author = {Tahmasebpour, M and Sotudeh-Gharebagh, R and Zarghami, R and Mostoufi, N},
isbn = {9788489925786},
journal = {Proceedings of the 4th International Conference on Computational Methods for Coupled Problems in Science and Engineering (COUPLED PROBLEMS 2011)},
keywords = {fluidized bed,pressure fluctuation,recurrence plot,recurrence quantification analysis},
mendeley-groups = {Recurrence Plots Library},
title = {{Recurrence plots analysis of pressure fluctuations in fluidized beds}},
year = {2012}
}
