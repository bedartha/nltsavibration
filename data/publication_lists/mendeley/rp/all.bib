@article{S.2015a,
abstract = {Introduction: As part of an ongoing Department of Defense double-blind, sham-controlled study of soldiers with mild-moderate Traumatic Brain Injury (mTBI) or TBI plus Posttraumatic Stress Disorder (PTSD) who received hyperbaric oxygen (or sham) for treatment of TBI, we report the sleep patterns of 62 male soldiers at baseline. Methods: Sleep was captured from the baseline time series of an ambulatory Holter-style device that combines a single-channel ECG with a three-channel accelerometer. This battery-operated device (CamN-Tech, London, England) captures variability in both heart-rate and movement. We compared the two groups of brain-injured soldiers (with and without PTSD) to each other as well as to a group of age and sex-matched normals. After collecting 24-hours of heart-rate and accelerometer data from each participant at baseline, we separated the sleep segment manually. Sleep was then studied using a commercially available program (NevroKard) for HRV that we adapted to our purpose, as well as through custom programs that we wrote. Sleep and sleep architecture were studied using heart-rate variability (HRV) and movement variability. Results: First, linear metrics of HRV in both time and frequency domains are described. We report results for the tachogram including segmented statistics, trend analysis, histogram, and Poincare plot and autoregressive spectrum. In addition we calculated nonlinear measures of HRV, including our new method for Poincare plot analysis which emphasizes the relationship of asymmetry in SD1 to these clinical conditions. We also made a movie of the recurrence plot of each soldier which we call a Dynamic Poincare. Next, we report nonlinear statistics for sleep such as Approximate Entropy (organization/disorganization of sleep) and Detrended Fluctuation Analysis (removal of short-term trends to see long-term pattern of the entire sleep time). Taken together, these measures characterize sleep and sleep architecture in TBI and TBI plus PTSD subjects and distinguish these two groups from each other as well as from normal controls. Conclusion: Our results conclude the usefulness of ambulatory HRV time series to characterize sleep and sleep disorders in a mobile population and the specificity of HRV to separate those with TBI from those with TBI plus PTSD. We hope that these measures will also separate treatment from non-treated participants.},
author = {S., Mirow and S.H., Wilson and S., Churchill and L.K., Weaver},
doi = {10.1109/MMSP.2002.1203314},
isbn = {0161-8105},
issn = {0161-8105},
journal = {Sleep},
keywords = {United Kingdom,accelerometer,ambulatory monitoring,architecture,audiovisual equipment,brain,controlled study,devices,electrocardiogram,entropy,heart rate,heart rate variability,histogram,hope,human,hyperbaric oxygen therapy,male,organization,population,posttraumatic stress disorder,sleep,sleep disorder,sleep pattern,sleep time,soldier,statistics,time series analysis,traumatic brain injury},
mendeley-groups = {Recurrence Plots Library},
title = {{Ambulatory monitoring of sleep in soldiers with mild/moderate traumatic brain injury through analysis of linear and nonlinear measures of heart-rate variability and movement variability}},
year = {2015}
}
@article{Du2015a,
abstract = {Based on the viewpoint that the properties of polymer could be changed by nano particles, epoxy/Al{\textless}sub{\textgreater}2{\textless}/sub{\textgreater}O{\textless}sub{\textgreater}3{\textless}/sub{\textgreater} nanocomposites were prepared to study the effects of nano particles on tracking failure. Nano-Al{\textless}sub{\textgreater}2{\textless}/sub{\textgreater}O{\textless}sub{\textgreater}3{\textless}/sub{\textgreater} particles were mixed into neat epoxy with filler content of 1, 3, 5 and 7 wt{\%}. Tracking experiments were carried out by applying positive dc voltage. The effects of the filler content on tracking resistance were studied by recording the tracking phenomena and discharge currents during the tracking process. Obtained results showed that when the filler content increased from 0 to 5 wt{\%}, the duration of the whole tracking process increased, the weight loss of nanocomposites and the area of tracking pattern decreased. The opposite tendency was observed when the content increased from 5 to 7 wt{\%}. The discharge currents were studied using the recurrence plot technique to investigate the nonlinear characteristics of discharge currents and reflect the effects of nano particles on discharge. The fractal dimension method was used to study the structures of scintillation discharge distribution and tracking pattern to analyze the effects of nano particles on the occurrence of tracking pattern. The results showed that the nano-Al{\textless}sub{\textgreater}2{\textless}/sub{\textgreater}O{\textless}sub{\textgreater}3{\textless}/sub{\textgreater} particles had very significant effects on the tracking resistance of epoxy. The best restriction effects were obtained with the filler content of 5 wt{\%}.},
author = {Du, B. and Guo, Y.},
doi = {10.1109/TDEI.2014.004520},
issn = {10709878},
journal = {IEEE Transactions on Dielectrics and Electrical Insulation},
keywords = {dc voltage,epoxy,nano-Al2O3particles,tracking failure,tracking pattern},
mendeley-groups = {Recurrence Plots Library},
title = {{Tracking resistance of epoxy/Al2O3nanocomposites under DC voltage}},
year = {2015}
}
