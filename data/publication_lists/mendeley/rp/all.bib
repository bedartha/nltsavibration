isbn = {9781467346221},
issn = {10915281},
keywords = {Performance degradation monitoring,autoregression model,prediction,recurrence quantification analysis},
mendeley-groups = {Recurrence Plots Library},
title = {{Bearing performance degradation evaluation using recurrence quantification analysis and auto-regression model}},
year = {2013}
}
@article{Wu2013a,
abstract = {The periodic transference of nucleotide strings in bacterial and archaeal complete genomes is investigated by using the metric representation and the recurrence plot method. The generated periodic correlation structures exhibit four kinds of fundamental transferring characteristics: a single increasing period, several increasing periods, an increasing quasi-period and almost noincreasing period. The mechanism of the periodic transference is further analyzed by determining all long periodic nucleotide strings in the bacterial and archaeal complete genomes and is explained as follows: both the repetition of basic periodic nucleotide strings and the transference of non-periodic nucleotide strings would form the periodic correlation structures with approximately the same increasing periods.},
archivePrefix = {arXiv},
arxivId = {1302.7102},
author = {Wu, Zuo-Bing},
doi = {10.2174/1574893611308020015},
eprint = {1302.7102},
isbn = {1574-8936},
issn = {15748936},
journal = {Current Bioinformatics},
mendeley-groups = {Recurrence Plots Library},
title = {{Periodic Correlation Structures in Bacterial and Archaeal Complete Genomes}},
year = {2013}
}
@article{NOMURA2013a,
abstract = {The field of vibration-based structural health monitoring involves extracting a feature which robustly quantifies damage-induced changes of the structural dynamics. Generally, the change of structural characteristics due to damage is analyzed by observing frequencies, mode shapes, damping, etc. However, the change of those modal features due to damage is minor, so that detecting the location of damage is difficult. Recently, an attractor-based monitoring system has been proposed. The recent study using chaotic excitation has demonstrated that the change of the attractor caused by damage is more sensitive than that of the most commonly used frequency and mode shape of the structure, in which the recurrence plot was applied successfully to the damage detection. However, damage localization has not been discussed sufficiently. An attempt in this study was made to develop a damage localization method using chaotic excitation and recurrence quantification analysis. It was demonstrated through experiment results that the proposed system makes it possible to detect damage locations.},
author = {NOMURA, Yasutoshi and MORIMOTO, Daiki and KUSAKA, Takayuki and FURUTA, Hitoshi},
doi = {10.1299/kikaic.79.4210},
issn = {1884-8354},
journal = {TRANSACTIONS OF THE JAPAN SOCIETY OF MECHANICAL ENGINEERS Series C},
mendeley-groups = {Recurrence Plots Library},
title = {{Structural Damage Localization Based on Recurrence Plots of Chaotic Response Attractor}},
year = {2013}
}
@inproceedings{Digulescu2013a,
abstract = {Modulation detection, one of the most important functions of a receiver in a communication system, is an intermediary step between its signal detection and demodulation. In this paper, we present the use of the recurrence plot analysis method (RPA) used for the detection of the modulation technique used for a wireless sensor network (WSN). We show a technique for the RPA method to detect ASK, FSK, even PSK signals using recurrence matrix or distance matrix [1]. We also show the potential to detect OFDM modulations using this method. Simulation results confirm the importance of the RPA method. {\textcopyright} 2013 NICT.},
author = {Digulescu, A. and Celik, E. T. and Serbanescu, A. and Fratu, O. and Halunga, S.},
booktitle = {International Symposium on Wireless Personal Multimedia Communications, WPMC},
issn = {13476890},
keywords = {ASK,FSK,ODFM,PSK,RPA,modulation detection},
mendeley-groups = {Recurrence Plots Library},
title = {{Modulation detection in wireless sensor networks using recurrence plot analysis}},
year = {2013}
}
@inproceedings{Feng2013a,
abstract = {The real sea clutter has been proved to be nonlinear and non-stationary. The meta recurrence plot (MRP) is a tool to analyze the non-stationary time series. So in this paper, the MRP is imported to analyze the real sea clutter data firstly. For quantifying the structures of the recurrence points in the MRPs, the recurrence quantification analysis (RQA) is also used. The results show the measures based on the RQA can detect the targets in the real sea clutter effectively.},
author = {Feng, C and Cheng, J and Zhou, L},
booktitle = {Proceedings of the Ninth International Conference on Natural Computation (ICNC2013)},
doi = {10.1109/ICNC.2013.6818143},
isbn = {9781467347143},
keywords = {mata recurrence plot,recurrence quantification analysis,sea clutter},
mendeley-groups = {Recurrence Plots Library},
title = {{Analysis of real sea clutter based on meta recurrence plot}},
year = {2013}
}
@article{DiMarco2013a,
abstract = {Background Successful termination of atrial fibrillation (AF) during catheter ablation (CA) is associated with arrhythmia-free follow-up. Preablation factors such as mean atrial fibrillation cycle length (AFCL) predict the likelihood of AF termination during ablation but recurring patterns and AFCL stability have not been evaluated. Objective To investigate novel predictors of acute and postoperative ablation outcomes from intracardiac electrograms: (1) recurring AFCL patterns and (2) localization index (LI) of the instantaneous fibrillatory rate distribution. Methods Sixty-two patients with AF (32 paroxysmal AF; 45 men; age 57 ± 10 years) referred for CA were enrolled. One-minute electrogram was recorded from coronary sinus (CS; 5 bipoles) and right atrial appendage (HRA; 2 bipoles). Atrial activations were detected automatically to derive the AFCL and instantaneous fibrillatory rate (inverse of AFCL) time series. Recurring AFCL patterns were quantified by using recurrence plot indices (RPIs): percentage determinism, entropy of determinism, and maximum diagonal length. AFCL stability was determined by using the LI. The CA outcome predictivity of individual indices was assessed. Results Patients with terminated atrial fibrillation (T-AF) had higher RPI (P {\textless}.05 in CS7-8) and LI than did those with nonterminated atrial fibrillation (P {\textless}.005 in CS3-4; P {\textless}.05 in CS5-6, CS7-8, and HRA). Patients free of arrhythmia after 3-month follow-up had higher RPI and LI (all P {\textless}.05 in CS7-8). All indices except percentage determinism predicted T-AF in CS7-8(area under the curve [AUC] ≥ 0.71; odds ratio [OR] ≥ 4.50; P {\textless}.05). The median AFCL and LI predicted T-AF in HRAD(AUC ≥ 0.75; OR ≥ 7.76; P {\textless}.05). The RPI and LI predicted 3-month follow-up in CS7-8(AUC ≥ 0.68; OR ≥ 4.17; P {\textless}.05). Conclusions AFCL recurrence and stability indices could be used in selecting patients more likely to benefit from CA. {\textcopyright} 2013 Heart Rhythm Society.},
author = {{Di Marco}, Luigi Yuri and Raine, Daniel and Bourke, John P. and Langley, Philip},
doi = {10.1016/j.hrthm.2013.06.007},
isbn = {1556-3871 (Electronic)$\backslash$r1547-5271 (Linking)},
issn = {15475271},
journal = {Heart Rhythm},
keywords = {Atrial fibrillation,Electrogram segmentation,Instantaneous fibrillatory rate distribution,Localization index,Recurrence quantification analysis},
mendeley-groups = {Recurrence Plots Library},
pmid = {23770069},
title = {{Characteristics of atrial fibrillation cycle length predict restoration of sinus rhythm by catheter ablation}},
year = {2013}
}
@inproceedings{Serbanescu2013a,
abstract = {This paper highlights the main results of our research in the field of recurrence plot analysis in the last five years. It briefly discusses results in the study of the method itself, as well as in some applications of it, i.e. characterization of speech signals, detection and localization of partial discharges in electric cables, representation of transient signals, estimation of signal parameters, reduction of additive noise, and representation of digitally modulated signals (ASK, FSK, PSK, and OFDM). {\textcopyright} 2013 IEEE.},
