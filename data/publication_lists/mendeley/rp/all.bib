isbn = {9781457720727},
keywords = {Acupuncture,EEG,Nonlinear,Phase Synchronization},
mendeley-groups = {Recurrence Plots Library},
title = {{Synchronization analysis of EEG signals evoked by manual acupuncture}},
year = {2012}
}
@article{Faszczewski2012a,
abstract = {In the paper the method based on recurrence plots has been used for identification of two-phase flow in a vertical, circular mini-channel. The time series obtained from image analysis of high speed video have been used. The method proposed in the present study allows us to define the coefficients which characterize the dynamics of two-phase flow in a mini channel. To identify two-phase flow patterns in a mini channel there has been used the coefficient LAVG which is a measure of an average length of diagonal lines in recurrence plot. The following two-phase flow structures have been considered: bubbly, bubbly-slug and wavy-annular. Obtained results show that method proposed in the paper enables identification of borders between two two-phase flow patterns coexisting in a mini-channel.},
author = {Faszczewski, Mariusz and G{\'{o}}rski, Grzegorz and Mosdorf, Romuald},
issn = {18984088},
journal = {Acta Mechanica et Automatica},
keywords = {Bubble Pump,Mini-Channel,Nonlinear Analysis,The Method of Recurrence Plots,Two-Phase Flow},
mendeley-groups = {Recurrence Plots Library},
title = {{Applying recurrence plots to identify borders between two-phase flow patterns in vertical circular mini channel}},
year = {2012}
}
@article{Wang2012b,
abstract = {It is proposed to introduce recurrence plot and recurrence quantification analysis into the research and processing of non-stationary power quality disturbance signals. By means of analyzing recurrence plot and recurrence quantification property, the recurrence quantification parameters are used to quantitatively characterize the recurrence characteristics of power quality disturbance signals. Using different parameters extracted by recurrence quantification analysis the disturbance training is carried out by BP neural network to discriminate various power quality disturbance modes. A characteristic parameter, namely the recursive degree of signal, is further chosen to preliminarily detect the endpoints of power quality disturbance signals. Simulation results show that the proposed method can recognize and locate transient disturbance signal such as voltage sag, voltage swell, voltage interruption, transient oscillation, voltage spike, and the recognition rate is higher than 98{\%}.},
author = {Wang, X and Bi, G and Zu, Z},
