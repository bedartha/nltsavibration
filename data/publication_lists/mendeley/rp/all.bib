@article{G2012a,
abstract = {Recurrence is a fundamental property of dynamical systems, which can$\backslash$nbe exploited to characterise the system's behaviour in phase space.$\backslash$nA powerful tool for their visualisation and analysis called recurrence$\backslash$nplot was introduced in the late 1980's. This report is a comprehensive$\backslash$noverview covering recurrence based methods and their applications$\backslash$nwith an emphasis on recent developments. After a brief outline of$\backslash$nthe theory of recurrences, the basic idea of the recurrence plot$\backslash$nwith its variations is presented. This includes the quantification$\backslash$nof recurrence plots, like the recurrence quantification analysis,$\backslash$nwhich is highly effective to detect, e.g., transitions in the dynamics$\backslash$nof systems from time series. A main point is how to link recurrences$\backslash$nto dynamical invariants and unstable periodic orbits. This and further$\backslash$nevidence suggest that recurrences contain all relevant information$\backslash$nabout a system's behaviour. As the respective phase spaces of two$\backslash$nsystems change due to coupling, recurrence plots allow studying and$\backslash$nquantifying their interaction. This fact also provides us with a$\backslash$nsensitive tool for the study of synchronisation of complex systems.$\backslash$nIn the last part of the report several applications of recurrence$\backslash$nplots in economy, physiology, neuroscience, earth sciences, astrophysics$\backslash$nand engineering are shown. The aim of this work is to provide the$\backslash$nreaders with the know how for the application of recurrence plot$\backslash$nbased methods in their own field of research. We therefore detail$\backslash$nthe analysis of data and indicate possible difficulties and pitfalls.$\backslash$n(c) 2006 Elsevier B.V. All rights reserved.},
author = {G, Patil Shilpa and G, Chonde Sonal and S, Jadhav Aasawari and D, Raut Prakash},
isbn = {9787030338945},
journal = {Res. J. Recent Sci},
keywords = {1,2,2012,56-60,earch journal of recent,feb,sciences {\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_},vol},
