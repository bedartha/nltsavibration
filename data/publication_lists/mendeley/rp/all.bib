@inproceedings{Du2007a,
abstract = {Chaos analysis of scintillation discharge based on tracking test of gamma-ray irradiated polyethylene terephthalate (PET) was presented. According to characteristics of statistical self-similarity and complexity of the discharge current, fractal dimensions were estimated and recurrence plots were reconstructed for the irradiated samples. Obtained results indicate that the fractal dimension decreases and the point density in recurrence plots increases with increasing the irradiated dosage. The radiation dosage effects on comparative tracking index (CTI) were also reported with increasing the total dosage of gamma-ray radiation. The variation of fractal dimension and point density of recurrence plot consists well with CTI. This study shows that the tracking resistance of PET increases with increasing the Gamma-ray radiation dosage.},
author = {Du, B. X. and Dong, D. S. and Zheng, X. L.},
