journal = {Movement Disorders},
keywords = {Parkinson disease,accelerometer,adult,cognition,disease course,eye,fox,gait,gender,height,human,kinematics,longitudinal study,marker,motor dysfunction,mutation,non profit organization,normal human,parameters,risk,velocity,weight},
mendeley-groups = {Recurrence Plots Library},
title = {{Postural control alterations in healthy LRRK2 G2019S mutation carriers}},
