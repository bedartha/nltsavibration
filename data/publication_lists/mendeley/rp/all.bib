@article{Rodkin2002a,
abstract = {Development of the faulting zone of an earthquake is modeled in terms of a fracture zone in a medium with a Cauchy law distribution of strength. The model yields a power law relation between the size of faulting zones and their number, constrains the characteristic average strength of a faulting zone as a function of its size, and indicates the presence of correlation between the average strength of faulting zones, recurrence plot slope, and dens3ity of the number of events. Some of the inferred model relations are known as characteristic features of the seismic regime; the extent to which the others are consistent with empirically derived characteristics of the seismic regime is discussed. The model results are interpreted within the framework of the fluid-metamorphic model of earthquake origin. Copyright {\textcopyright} 2002 by MAIK "Nauka/Interperiodica" (Russia).},
author = {Rodkin, M. V.},
issn = {10693513},
journal = {Izvestiya, Physics of the Solid Earth},
