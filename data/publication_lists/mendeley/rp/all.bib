@inproceedings{Thiel2006a,
abstract = {In this paper we show that delay embedding produces spurious structures$\backslash$nin a recurrence plot (RP) that are not present in the real attractor.$\backslash$nWe analyze typical sets of simulated data, such as white noise and$\backslash$ndata from the chaotic Rossler system to show the relevance of this$\backslash$neffect. In the second part of the paper we show that the second order$\backslash$nRenyi entropy and the correlation dimension are dynamical invariants$\backslash$nthat can be estimated from Recurrence Plots with arbitrary embedding$\backslash$ndimension and delay.},
