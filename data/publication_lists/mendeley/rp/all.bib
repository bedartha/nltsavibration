title = {{Caract{\'{e}}risation des signaux transitoires par l'analyse des r{\'{e}}currences de phase}},
year = {2011}
}
@inproceedings{Wang2011c,
abstract = {Recurrence plot and recurrence quantification analysis (RQA) are introduced in the research of nonstationary power disturbance signal process in this thesis. Through analyzing recurrence plot of power quality disturbance signal, the recurrence phenomena are observed within different patterns of PDS internal dynamic mechanism. The recurrence characteristics are shown by RQA parameters quantitatively. With the various parameters taken through RQA, the different power disturbance patterns are differentiated. For PDS end point inspection, signal recurrence level is the characteristic parameter which is used for measuring the recurrence frequency of vector points and the aggregation level of the tracks in system phase space. Based upon this parameter, the differences of power disturbance dynamic characteristics are analyzed, and the PDS end points are also inspected. The result of simulation experiments shows that the identification and positioning of various transient disturbance signals {\textcopyright} 2011 Published by Elsevier Ltd.},
author = {Wang, Xi and Bi, Gui Hong and Chen, Shi Long and Zu, Zhe},
booktitle = {Procedia Engineering},
doi = {10.1016/j.proeng.2011.08.772},
issn = {18777058},
keywords = {Phase-space reconstruction,Power quality,Quantitative recurrence analysis,Recursive map},
mendeley-groups = {Recurrence Plots Library},
title = {{A method to analyze power system quality disturbing signal based on recurrence quantification analysis}},
year = {2011}
}
@article{Xiao2011a,
