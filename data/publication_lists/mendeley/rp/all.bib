doi = {10.1142/S0129183105007492},
eprint = {0412765},
issn = {0129-1831},
journal = {International Journal of Modern Physics C},
mendeley-groups = {Recurrence Plots Library},
primaryClass = {cond-mat},
title = {{RECURRENCE PLOT AND RECURRENCE QUANTIFICATION ANALYSIS TECHNIQUES FOR DETECTING A CRITICAL REGIME.}},
year = {2005}
}
