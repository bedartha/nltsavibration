journal = {Arxiv preprint cond-mat/0412765},
keywords = {RP,abstract,analysis,and recurrence quantification analysis,are signal numerical analysis,dax,endogenous crash,financial bubble,methodologies able to work,nasdaq,nonlinear time series analysis,recurrence plot,recurrence quantification,rp,rqa,stock,with non linear dynam-},
