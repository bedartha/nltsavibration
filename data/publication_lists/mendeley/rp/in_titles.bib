isbn = {9781467377485},
keywords = {Polymeric insulators,determinism,erosion,recurrence plot,tracking},
mendeley-groups = {RP In Titles},
title = {{Recurrence plot analysis for assessing the quality of polymeric insulators}},
year = {2016}
}
@inproceedings{Johari2016c,
abstract = {A robust target recognition method proposed based on recurrence plot and recurrence quantification analysis (RQA) to generate robust features against noise, target velocity and aspect angle from micro-Doppler (m-D) signatures. The proposed method is tested on simulated data of three different targets using multiclass support vector machine (MSVM) and classification rate of about 95 {\%} is achieved. Also, effect of noise and coherent processing time (CPT) on classification rate is investigated.},
author = {Johari, Mohammad Mahdi and Nayebi, Mohammad Mahdi},
booktitle = {Proceedings International Radar Symposium},
doi = {10.1109/IRS.2016.7497362},
