keywords = {Bayes procedures,Dynamics,Hidden Markov Models,Learning systems,Speech recognition},
mendeley-groups = {RP In Titles},
title = {{A nonlinear dynamic modelling for speech recognition using Recurrence plot - A dynamic Bayesian approach}},
year = {2007}
}
@inproceedings{Terrill2007c,
abstract = {Recurrence plot analysis is a useful non-linear analysis tool. There are still no well formalised procedures for carrying out this analysis on measured physiological data, and systemising analysis is often difficult. In this paper, the recurrence based embedding is compared to radius based embedding by studying a logistic attractor and measured breathing data collected from sleeping human infants. Recurrence based embedding appears to be a more robust method of carrying out a recurrence analysis when attractor size is likely to be different between datasets. In the infant breathing data, the radius measure calculated at a fixed recurrence, scaled by average respiratory period, allows the accurate discrimination of active sleep from quiet sleep states (AUC=0.975, Sn=098, Sp=0.94).},
author = {Terrill, Philip I. and Wilson, Stephen and Suresh, Sadasivam and Cooper, David M.},
