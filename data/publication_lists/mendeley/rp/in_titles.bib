@article{Wendi2018e,
abstract = {One main challenge in constructing a reliable recurrence plot (RP) and, hence, its quantification [recurrence quantification analysis (RQA)] of a continuous dynamical system is the induced noise that is commonly found in observation time series. This induced noise is known to cause disrupted and deviated diagonal lines despite the known deterministic features and, hence, biases the diagonal line based RQA measures and can lead to misleading conclusions. Although discontinuous lines can be further connected by increasing the recurrence threshold, such an approach triggers thick lines in the plot. However, thick lines also influence the RQA measures by artificially increasing the number of diagonals and the length of vertical lines [e.g., Determinism ( D E T) and Laminarity ( L A M) become artificially higher]. To take on this challenge, an extended RQA approach for accounting disrupted and deviated diagonal lines is proposed. The approach uses the concept of a sliding diagonal window with minimal window si...},
author = {Wendi, Dadiyorto and Marwan, Norbert},
doi = {10.1063/1.5025485},
issn = {10541500},
journal = {Chaos},
mendeley-groups = {RP In Titles},
title = {{Extended recurrence plot and quantification for noisy continuous dynamical systems}},
year = {2018}
}
