@article{Liu2013c,
abstract = {The three most widely used methods for reconstructing the underlying time series via the recurrence plots (RPs) of a dynamical system are compared with each other in this paper. We aim to reconstruct a toy series, a periodical series, a random series, and a chaotic series to compare the effectiveness of the most widely used typical methods in terms of signal correlation analysis. The application of the most effective algorithm to the typical chaotic Lorenz system verifies the correctness of such an effective algorithm. It is verified that, based on the unthresholded RPs, one can reconstruct the original attractor by choosing different RP thresholds based on the Hirata algorithm. It is shown that, in real applications, it is possible to reconstruct the underlying dynamics by using quite little information from observations of real dynamical systems. Moreover, rules of the threshold chosen in the algorithm are also suggested.},
author = {Liu, Jie and Shi, Shu Ting and Zhao, Jun Chan},
doi = {10.1088/1674-1056/22/1/010505},
issn = {16741056},
journal = {Chinese Physics B},
keywords = {chaotic system,correlation analysis,recurrence plot,time series analysis},
mendeley-groups = {RP In Titles},
title = {{Comparison study of typical algorithms for reconstructing time series from the recurrence plot of dynamical systems}},
year = {2013}
}
@inproceedings{Digulescu2013c,
abstract = {Modulation detection, one of the most important functions of a receiver in a communication system, is an intermediary step between its signal detection and demodulation. In this paper, we present the use of the recurrence plot analysis method (RPA) used for the detection of the modulation technique used for a wireless sensor network (WSN). We show a technique for the RPA method to detect ASK, FSK, even PSK signals using recurrence matrix or distance matrix [1]. We also show the potential to detect OFDM modulations using this method. Simulation results confirm the importance of the RPA method. {\textcopyright} 2013 NICT.},
