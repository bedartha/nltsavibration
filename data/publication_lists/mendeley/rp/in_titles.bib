journal = {Applied Mechanics and Materials},
keywords = {Engine Angular Speed,Gasoline Engine,Ignition Angle,Phase Space,Recurrence Plot Analysis (RPA)},
mendeley-groups = {RP In Titles},
title = {{Recurrence plot analysis to study parameters of a gasoline engine}},
year = {2017}
}
